{
  "version": 2,
  "name": "golden-passage-api",
  "builds": [
    {
      "src": "src/server.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/server.ts",
      "headers": {
        "Access-Control-Allow-Origin": "https://golden-passage-frontend.vercel.app",
        "Access-Control-Allow-Methods": "GET, POST, PUT, DELETE, OPTIONS, PATCH",
        "Access-Control-Allow-Headers": "Content-Type, Authorization, X-Requested-With, Accept",
        "Access-Control-Allow-Credentials": "true"
      }
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "FRONTEND_URL": "https://golden-passage-frontend.vercel.app"
  },
  "github": {
    "enabled": false
  }
}
